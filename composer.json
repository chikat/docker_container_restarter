        "php": ">=7.2.6",
        "slim/slim": "^3.1",
        "docker-php/docker-php": "1.24.0"
    },
    "require-dev": {
        "phpunit/phpunit": ">=4.8 < 6.0"
    },
    "autoload": {
        "psr-4": {
